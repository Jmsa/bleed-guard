{
  "name": "bleed-guard",
  "version": "1.0.0",
  "description": "A collection of test reporters dedicated ot helping track, and stop, test bleed.",
  "repository": "https://github.com/Jmsa/bleed-guard.git",
  "author": "James Abercrombie <jmsabercrombie88@gmail.com>",
  "license": "MIT",
  "keywords": [
    "test",
    "reporter",
    "window",
    "global",
    "dom",
    "bleed",
    "leak"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "reporters/jest/jest.js"
  ],
  "scripts": {
    "test:reporter:jest": "yarn jest ./reporters/jest/jest.spec.js",
    "test:example:jest": "yarn jest ./examples/jest/*.spec.js -c ./examples/jest/jest.config.js",
    "test:reporter:vitest": "yarn vitest ./reporters/vitest/vitest.spec.ts",
    "test:example:vitest": "yarn vitest ./examples/vitest/*.spec.js -c ./examples/vitest/vitest.config.js",
    "release": "standard-version"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/svelte": "^5.2.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jquery": "^3.7.1",
    "jsdom": "^25.0.1",
    "mock-fs": "^5.2.0",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "deep-object-diff": "^1.1.9"
  }
}
